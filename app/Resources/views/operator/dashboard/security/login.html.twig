<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}

        {% stylesheets
        'assets/vendor/bootstrap/dist/css/bootstrap.min.css'
        'bundles/app/pixeladmin/assets/stylesheets/pixel-admin.min.css'
        'bundles/app/pixeladmin/assets/stylesheets/pages.min.css'
        'bundles/app/pixeladmin/assets/stylesheets/rtl.min.css'
        'bundles/app/pixeladmin/assets/stylesheets/themes.min.css'
        'assets/vendor/font-awesome/css/font-awesome.css'
        'bundles/app/stylesheets/operator/dashboard/security/login/style.css'
        %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
    {% endblock %}
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body dir="rtl" class="theme-default page-signin-alt">

{% if error %}
    <div class="alert alert-danger">
        <button type="button" class="close" data-dismiss="alert">×</button>
        {{ error.messageKey|trans(error.messageData, 'security') }}<
    </div>
{% endif %}

<div class="signin-header">
    <a href="index.html" class="logo">
        <div class="demo-logo bg-primary"><img src="assets/demo/logo-big.png" alt="" style="margin-top: -4px;"></div>&nbsp;
        <strong>Roapp</strong>
    </a> <!-- / .logo -->
</div> <!-- / .header -->
<h1 class="form-header">ورود به سامانه</h1>
<!-- Form -->
<form action="{{ path('app_operator_dashboard_security_login') }}" id="signin-form_id" class="panel" method="post">
    <div class="form-group">
        <input type="text" value="{{ last_username }}" name="_username" id="username" class="form-control input-lg" placeholder="پست الکترونیکی">
        <input type="hidden" name="_target_path" value="{{ url('app_operator_dashboard_shipment_list') }}" />
    </div> <!-- / Username -->

    <div class="form-group signin-password">
        <input type="password" name="_password" id="password" class="form-control input-lg" placeholder="رمز عبور">
        <a href="#" class="forgot">فراموشی?</a>
    </div> <!-- / Password -->

    <div class="form-actions">
        <input type="submit" value="ورود" class="btn btn-primary btn-block btn-lg">
    </div> <!-- / .form-actions -->
</form>
<!-- / Form -->
</body>